<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="info.repy.adoptopenjdkupdate.Controller">
    <columnConstraints>
        <ColumnConstraints minWidth="10.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" />
        <RowConstraints minHeight="10.0" />
        <RowConstraints minHeight="10.0" />
        <RowConstraints minHeight="10.0" />
        <RowConstraints minHeight="10.0"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <Label text="Distribution" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
        <ComboBox fx:id="distributionComboBox" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
        <Label text="Architecture" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
        <ComboBox fx:id="architectureComboBox" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
        <Label text="OS" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
        <ComboBox fx:id="osComboBox" GridPane.rowIndex="2" GridPane.columnIndex="1"/>
        <Label text="Version" GridPane.rowIndex="3" GridPane.columnIndex="0"/>
        <ComboBox fx:id="versionComboBox" GridPane.rowIndex="3" GridPane.columnIndex="1"/>
        <Button mnemonicParsing="false" onAction="#download" text="Download" GridPane.rowIndex="4"
                GridPane.columnIndex="1"/>
        <TextArea fx:id="textArea" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                  GridPane.rowIndex="5" GridPane.columnSpan="2"/>
    </children>
</GridPane>
